{
  "name": "clean-wallet-web",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "lint": "eslint src server",
    "dev": "react-app-rewired start",
    "start": "npm-run-all --parallel start:*",
    "start:client": "npm run dev",
    "start:server": "NODE_ENV=development nodemon server/index.js -w server",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run bundle:client && npm run bundle:server",
    "bundle:client": "PUBLIC_URL='/.blocklet/proxy/z8iZxKtyeGLaXH4LkQ7Dxk7D86i1M1gQvfjVx' npm run build",
    "bundle:server": "npm run clean && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
